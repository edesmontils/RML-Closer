@prefix log: <http://www.w3.org/2000/10/swap/log#>.@prefix str: <http://www.w3.org/2000/10/swap/string#>.@prefix owl: <http://www.w3.org/2002/07/owl#>.@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.@prefix rr: <http://www.w3.org/ns/r2rml#> .@prefix rml: <http://semweb.mmlab.be/ns/rml#> .@prefix ql: <http://semweb.mmlab.be/ns/ql#> .@prefix : <http://github.com/edesmontils/RML-Closer#> .# ===========================================================================================# Gestion de la subsomption des concepts# ===========================================================================================[(?m rr:subjectMap ?s) (?s rr:class ?C) (?C rdfs:subClassOf ?D) -> (?s rr:class ?D)]# sameAs : discutable... peut-être perturbant car du coup on a 2 fois la même connaissance ![(?m rr:subjectMap ?s) (?s rr:class ?C) (?C owl:sameAs ?D) -> (?s rr:class ?D)]# ===========================================================================================# Gestion de la subsomption des relations# ===========================================================================================[(?m rr:predicateObjectMap ?s) (?s rr:predicate ?p) (?p rdfs:subPropertyOf ?q)-> (?s rr:predicate ?q)]# ===========================================================================================# Gestion des caractéristiques des propriétés# ===========================================================================================# {?P owl:inverseOf ?Q. ?S ?P ?O} => {?O ?Q ?S}.[(?P owl:inverseOf ?Q)(?m1 rr:predicateObjectMap ?s1) (?s1 rr:predicate ?P) (?m2 rr:predicateObjectMap ?s2) noValue(?s2 rr:predicate ?Q)->]# {?P a owl:SymmetricProperty. ?S ?P ?O} => {?O ?P ?S}.# Transitive prop. : pas gérable à ce niveau ! # Devrait être fait lors de la construction de la requête interne (TP2Query) ou après...# {?P a owl:TransitiveProperty. ?X ?P ?O. ?S ?P ?X} => {?S ?P ?O}.#end